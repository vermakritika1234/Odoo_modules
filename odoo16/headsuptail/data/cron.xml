<odoo>
    <data>
        <record id="ir_cron_calculate_duration" model="ir.cron">
            <field name="name">Calculate Duration Cron</field>
            <field name="model_id" ref="model_students_students"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.calculate_duration()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>

         <record id="ir_cron_calculate_duration_teacher" model="ir.cron">
            <field name="name">Calculate Duration Cron for teacher</field>
            <field name="model_id" ref="model_teacher_teacher"/>
            <field name="type">ir.actions.server</field>
            <field name="state">code</field>
            <field name="code">model.calculate_duration_teacher()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
