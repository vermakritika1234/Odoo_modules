<odoo>
  <data>
 <record model="ir.ui.view" id="books_books">
      <field name="name">books.books</field>
      <field name="model">books.books</field>
      <field name="arch" type="xml">
        <form>
         <sheet>
          <!-- <header>
           <button name="%(action_wizardxml_to_viewxml)d" type="action" string="Wizard button" data-hotkey="w"/>
             </header> -->
           <group>
              <field name="name"/>
              <field name="code"/>
          </group>
          </sheet>
        </form>
      </field>
    </record>

     <record model="ir.ui.view" id="book_books_tree">
      <field name="name">books.books</field>
      <field name="model">books.books</field>
      <field name="arch" type="xml">
        <tree>
              <field name="name"/>
              <field name="code"/>
          </tree>
      </field>
    </record>

     <record model="ir.actions.act_window" id="action_create_books_books">
      <field name="name">Create Books</field>
      <field name="res_model">books.books</field>
      <field name="view_mode">tree,form</field>
    </record>
   

    <!-- <menuitem name="My menu" id="headsuptail.menu_root2"/>

      <menuitem name="Books" id="books_books" sequence="1"  action="action_create_books_books"/> -->
  </data>
</odoo>