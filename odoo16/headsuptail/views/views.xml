  <record model="ir.ui.view" id="user_registration">
        <field name="name">User Registration</field>
        <field name="model">user.registration</field>
        <field name="arch" type="xml">
          <form>
          <sheet>
            <header>
            <button name="%(action_wizardxml_to_viewxml)d" type="action" string="Wizard button" data-hotkey="w"/>
              </header>
            <group>
                <field name="name"/>
                <field name="age"/>
                <field name="designation"/>
                <field name="books_record_ids" editable='bottom'/>
                <field name="books_data_ids"/>
            </group>
            </sheet>
          </form>
        </field>
      </record>

      <record model="ir.ui.view" id="user_registration_tree">
        <field name="name">User Registration Tree</field>
        <field name="model">user.registration</field>
        <field name="arch" type="xml">
          <tree>
                <field name="name"/>
                <field name="age"/>
                <field name="designation"/>
                <field name="books_record_ids"/>
                <field name="books_data_ids"/>

            </tree>
        </field>
      </record>

      <record model="ir.actions.act_window" id="action_user_register">
        <field name="name">headsuptail window</field>
        <field name="res_model">user.registration</field>
        <field name="view_mode">tree,form</field>
      </record>
    

      <menuitem name="headsuptail" id="headsuptail.menu_root"/>

        <menuitem name="User Registration" id="register" parent="headsuptail.menu_root"
                action="action_user_register"/>

        
        <menuitem name="Books" id="create_books" parent="headsuptail.menu_root"
                action="action_create_books_books"/>

        <menuitem name="Students" id="create_students_students" parent="headsuptail.menu_root"
                action="action_create_students_students"/>

        
        <menuitem name="Teacher" id="create_teacher_teacher" parent="headsuptail.menu_root"
                action="action_create_teacher_teacher"/>
    </data>
