    <record id="custom_model_form" model="ir.ui.view">
        <field name="name">custom.form.model</field>
        <field name="model">custom.model</field>
        <field name="arch" type="xml">
            <form string="Custom model">
                <sheet name='User Profile'>
                <group>
                            <field name="name"/>
                            <field name="age"/>
                            <!-- <field name="sale_partner_id"/> -->
                </group>
             </sheet>
            </form>
        </field>
    </record>

    <record id="custom_model_tree" model="ir.ui.view">
        <field name="name">custom.model.tree</field>
        <field name="model">custom.model</field>
        <field name="arch" type="xml">
            <tree string="Variant Values">
                    <field name="name" />
                            <field name="age"/>
                            <!-- <field name="sale_partner_id"/> -->
            </tree>
        </field>
    </record>


    <record id="custom_model_action" model="ir.actions.act_window">
        <field name="name">User profile</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">custom.model</field>
        <field name="view_mode">tree,form</field>
    </record>

    
